{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to local Node.js",
      "port": 9229,
      "protocol": "inspector"
    },

    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Docker Node.js",
      "port": 9229,
      "address": "localhost",
      "restart": true,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/usr/app/",
      "protocol": "inspector"
    }
  ]
}
